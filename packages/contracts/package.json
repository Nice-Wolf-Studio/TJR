{
  "name": "@tjr/contracts",
  "version": "0.1.0",
  "description": "Canonical types, DTOs, and error classes for TJR trading system",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
